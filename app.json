{
  "expo": {
    "name": "Gridpay Terminal",
    "slug": "Gridpay Terminal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/thisIcon.png",
    "primaryColor":"#efefefef",
    "splash": {
      "image": "./assets/openScreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "@stripe/stripe-terminal-react-native",
        {
          "bluetoothBackgroundMode": true,
          "locationWhenInUsePermission": "Location access is required in order to accept payments.",
          "bluetoothPeripheralPermission": "Bluetooth access is required in order to connect to supported bluetooth card readers.",
          "bluetoothAlwaysUsagePermission": "This app uses Bluetooth to connect to supported card readers."
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "net.gridpay"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/thisAdaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "net.gridpay"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
